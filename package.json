{
  "name": "express-restfulapi-starter",
  "version": "1.0.0",
  "description": "Starter for restful api with express",
  "main": "./dist/server.js",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist",
    "dev": "nodemon -w src --exec babel-node src/server.js",
    "serve": "build && node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanhhtan/express-restfulapi-starter.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "babel"
  ],
  "author": "Hai Hua (Ryan)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryanhhtan/express-restfulapi-starter/issues"
  },
  "homepage": "https://github.com/ryanhhtan/express-restfulapi-starter#readme",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "babel-node": "^0.0.1-security",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}
